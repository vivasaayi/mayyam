{
  "name": "mayyam-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Mayyam - A comprehensive toolbox for DevOps and SRE engineers",
  "scripts": {
    "start": "react-scripts start",
    "start:port": "PORT=$PORT react-scripts start",
    "start:backend": "REACT_APP_BACKEND_HOST=localhost REACT_APP_BACKEND_PORT=$BACKEND_PORT react-scripts start",
    "start:dev": "REACT_APP_BACKEND_HOST=localhost REACT_APP_BACKEND_PORT=3000 react-scripts start",
    "start:dev:3005": "REACT_APP_BACKEND_HOST=localhost REACT_APP_BACKEND_PORT=3005 react-scripts start",
    "start:custom": "REACT_APP_BACKEND_HOST=localhost REACT_APP_BACKEND_PORT=${BACKEND_PORT:-3000} PORT=${FRONTEND_PORT:-3001} react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen localhost:3000",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Rajan Panneer Selvam",
  "license": "MIT",
  "dependencies": {
    "@coreui/chartjs": "^4.0.0",
    "@coreui/coreui": "^5.3.2",
    "@coreui/icons": "^3.0.1",
    "@coreui/react": "^5.6.0",
    "@coreui/react-chartjs": "^3.0.0",
    "ag-grid-community": "^29.3.5",
    "ag-grid-react": "^29.3.5",
    "axios": "^1.9.0",
    "chart.js": "^4.4.9",
    "classnames": "^2.5.1",
    "http-proxy-middleware": "^2.0.6",
    "jwt-decode": "^4.0.0",
    "papaparse": "^5.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-json-view": "^1.21.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.0",
    "react-scripts": "^5.0.1",
    "reactstrap": "^9.2.3",
    "sass": "^1.88.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0"
  }
}
